{
  "name": "hb-back",
  "version": "0.0.1",
  "description": "Helpbuttons backend",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn eslint && yarn prettier:check",
    "lint:fix": "yarn eslint:fix && yarn prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn prettier:cli -- -l",
    "prettier:fix": "yarn prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn eslint -- --fix",
    "pretest": "yarn rebuild",
    "test": "mocha ./dist/__tests__/**/*.test.js",
    "test:memory": "NODE_ENV=memory mocha ./dist/__tests__/**/*.test.js",
    "posttest": "yarn lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn posttest",
    "docker:build": "docker build -t hb-back .",
    "docker:run": "docker run -p 3000:3000 -d hb-back",
    "premigrate": "yarn build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn rebuild",
    "start": "node -r source-map-support/register .",
    "start:memory": "NODE_ENV=memory node -r source-map-support/register .",
    "start:watch": "tsc-watch --target es2017 --outDir ./dist --onSuccess \"node .\"",
    "start:memory:watch": "NODE_ENV=memory tsc-watch --target es2017 --outDir ./dist --onSuccess \"node .\"",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn clean && yarn build",
    "db:start": "docker-compose up -d db",
    "db:term": "docker-compose exec db psql -U postgres",
    "db:watch": "docker-compose logs -f --tail=30"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Albatroz Jeremias <ajeremias@coletivos.org>",
  "license": "AGPL-3.0-only",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^7.3.2",
    "@loopback/authentication-jwt": "^0.9.2",
    "@loopback/authorization": "^0.9.2",
    "@loopback/boot": "^3.4.2",
    "@loopback/core": "^2.16.2",
    "@loopback/repository": "^3.7.1",
    "@loopback/rest": "^10.0.0",
    "@loopback/rest-explorer": "^3.3.2",
    "@loopback/service-proxy": "^3.2.2",
    "@types/geojson": "^7946.0.8",
    "@types/multer": "^1.4.7",
    "dotenv": "^10.0.0",
    "geojson-validation": "^1.0.2",
    "loopback-connector-postgresql": "^5.4.0",
    "moment": "^2.29.1",
    "multer": "^1.4.3",
    "nodemailer": "^6.6.5",
    "tslib": "^2.0.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@loopback/build": "^7.0.0",
    "@loopback/eslint-config": "^11.0.0",
    "@loopback/testlab": "^3.4.2",
    "@types/node": "^10.17.60",
    "eslint": "^7.29.0",
    "source-map-support": "^0.5.19",
    "tsc-watch": "^4.5.0",
    "typescript": "~4.3.4"
  }
}
